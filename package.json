{
  "name": "vue-plugin-events",
  "version": "1.0.0",
  "description": "Simple global event bus for Vue.js applications",
  "author": "Ricardo Nolde <ricardo@nolde.com.br>",
  "main": "src/index.js",
  "module": "dist/vue-plugin-events.esm.js",
  "browser": "dist/vue-plugin-events.umd.js",
  "files": [
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "browser-env": "3.2.6",
    "esm": "3.2.13",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "prettier-standard": "9.1.1",
    "rollup": "1.4.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-license": "^0.8.1",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-replace": "2.1.0",
    "sinon": "^7.2.7",
    "tap-spec": "^5.0.0",
    "tape": "4.10.1",
    "vue": "2.6.8"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "tape -r esm -r ./test/env ./test/**/*.test.js | tap-spec",
    "format": "prettier-standard ./src/**/*.js"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "standard": {
    "ignore": [
      "*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nolde/vue-plugin-events.git"
  },
  "bugs": {
    "url": "https://github.com/nolde/vue-plugin-events/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "vue2",
    "vue-plugin",
    "plugin",
    "vuejs2",
    "events",
    "event-bus",
    "rollup",
    "javascript",
    "browser"
  ],
  "license": "ISC",
  "homepage": "https://github.com/nolde/vue-plugin-events#readme"
}
